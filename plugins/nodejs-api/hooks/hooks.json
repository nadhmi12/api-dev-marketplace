{
  "hooks": {
    "user-prompt-submit-hook": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo 'Node.js API plugin: Processing user request'",
            "timeout": 1000
          }
        ]
      }
    ],
    "before-tool-use-hook": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo 'Node.js API plugin: About to execute tool'",
            "timeout": 500
          }
        ]
      }
    ]
  }
}
